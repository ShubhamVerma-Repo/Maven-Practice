<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>  
  <servlet>
  	<servlet-name>MyFirstServlet</servlet-name>
  	<display-name>MyFirstServlet</display-name>
  	<description></description>
  	<servlet-class>com.demo.MyFirstServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>MySecondServlet</servlet-name>
  	<display-name>MySecondServlet</display-name>
  	<description></description>
  	<servlet-class>com.demo.MySecondServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>MyFirstServlet</servlet-name>
  	<url-pattern>/MyFirstServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>MySecondServlet</servlet-name>
  	<url-pattern>/MySecondServlet</url-pattern>
  </servlet-mapping>
</web-app>
